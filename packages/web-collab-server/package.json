{
  "name": "@foxitsoftware/web-collab-server",
  "version": "1.4.0",
  "description": "Server module for Foxit Web Collaboration Add-on",
  "private": true,
  "license": "Commercial",
  "main": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-ws": "^9.0.11",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/websockets": "^9.0.11",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cuid": "^2.1.8",
    "dotenv": "16.0.1",
    "jsonwebtoken": "^8.5.1",
    "lib0": "0.2.58",
    "nestjs-pino": "^3.3.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.1",
    "qs": "^6.9.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "webpack": "^5.73.0",
    "ws": "^8.7.0",
    "yjs": "13.5.44",
    "@foxitsoftware/collab-db": "1.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "rollup-plugin-replace": "^2.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "scripts": {
    "build": "rimraf dist build collab-server-instrumented && nest build && rollup -c && rollup -c rollup.bundle.config.js",
    "build:cov": "rimraf collab-server-instrumented && nyc instrument --compact=false src collab-server-instrumented/src",
    "clean": "rimraf dist build collab-server-instrumented coverage-report",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "rimraf build && nest start",
    "start:dev": "rimraf build && nest start --watch",
    "start:debug": "rimraf build nest start --debug --watch",
    "start:prod": "node build/main",
    "start:cov": "nyc ts-node collab-server-instrumented/src/main.ts",
    "report:cov": "nyc report --reporter=html --report-dir coverage-report",
    "report:cobertura": "nyc report --reporter=cobertura --report-dir coverage-report-cobertura",
    "start:e2e": "run-s build:cov start:cov",
    "docs": "typedoc --excludePrivate --disableSources --tsconfig ./tsconfig.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  }
}